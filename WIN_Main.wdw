     name : CreateTreeStructure
     internal_properties : CAAAAAgAAACK5Zbgb96P6SR0m7lGWAPaleLOIjZVETb+wACZyV6yN4Adq7pJONTp7paC7e6WNQ5s3Lz1tg6105gULa4k5Aw9twj6ImxKb2oGXupoStv0Z3RJJ8+7nCE+yueCfLFSrJEhgBedYhfD/A6pd8jJQUy12piuG7WeeZ5Rji3ZvlY253NNdefEx/ZTCWaIcNXRlMnLpLe2aWI=
     procedure_id : 1219431843448630096
     type_code : 14
     code : |1+
      // Summary: Directory whose content must be listed
      // Syntax:
      //CreateTreeStructure (<sPath> is string)
      //
      // Parameters:
      //	sPath (string): <specify the role of sPath>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procedure CreateTreeStructure(sPath is string)
      
      // Variable
      gsDrivePath = sPath
      gsDrivePath = UncompleteDir(gsDrivePath)
      
      // Temporization Window
      OpenChild(WIN_Wait)
      WIN_Wait.STC_FILE = sPath
      
      // Reinitialize the controls
      TableDeleteAll(TVT_FileTable)
      TreeMapDeleteAll(TMAP_File)
      
      // Create the root
      TableAddLine(TVT_FileTable, gsDrivePath)
      
      // Find the files and sub-directories
      fListFile(CompleteDir(gsDrivePath) + “*.*”, FileList, Null, fdInterruptible)
      
      // Expand all
      TableExpandAll(TVT_FileTable)
      
      Close(WIN_Wait)
      
      
     type : 458752
   -
     name : GetPath
     procedure_id : 1219431843448695632
     type_code : 14
     code : |1+
      // Sumary: Retrieves the path from a subscript of the TreeMap
      // Syntax:
      // [ <Result> = ] GetPath (<nSubscript>)
      //
      // Parameters:
      // 	nSubscript: <specify the role of nSubscript>
      // Return value:
      // 	string: <specify the possible values as well as their interpretation>
      //
      // Example:
      // Indicate an example.
      //
      Procedure GetPath(LOCAL nSubscript)
      
      sPath is string = TVT_FileTable.COL_NAME[nSubscript]
      
      WHILE nSubscript <> 0 AND nSubscript <> -1
      	// Search for the subscript of the parent
      	nSubscript = TableGiveParent(TVT_FileTable, nSubscript)
      	IF nSubscript <> 0 AND nSubscript <> -1 THEN
      		sPath = TVT_FileTable.COL_NAME[nSubscript] + [“\”] + sPath
      	END
      END
      
      // Ignore the <Files> directory that is just used visually
      sPath = Replace(sPath, “\<Files>”, “”)
      
      RESULT sPath
     type : 458752
   -
     name : FileList
     internal_properties : CAAAAAgAAABKXJr3lgFN6X++thTbIlm9ODbHVEqlke21+pBjA8PSjgQnmUiCKbmDW1eM6AWqKLJZxNQ9GzD1swb8bzpNIIi652nFwVQBCM3pAioioF/T6StdVMR3Nr4OfU63AHtCawHe4KgO3v923neupqKnWdmVfxgVrh0aJHrcRXqvkEZzM15d5giiNaPN2AmpME3T+n1q0wLbaw==
     procedure_id : 1219431843448761168
     type_code : 14
     code : |1+
      // Summary: Callback called by fListFile
      // Syntax:
      // FileList (<sPath>, <sFileName>, <nChange>)
      //
      // Parameters:
      // 	sPath: <specify the role of sPath>
      // 	sFileName: <specify the role of sFileName>
      // 	nChange: <specify the role of nChange>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procedure FileList(sPath, sFileName, nChange)
      
      // Variables
      sParent, sTreestructure is string
      nSubscript is int
      
      Multitask(-1)
      WIN_Wait.STC_FILE = sPath + sFileName
      
      SWITCH nChange
      	CASE flFirstFile
      		// Add the "<Files>" directory
      		TableAddChild(TVT_FileTable, gsDrivePath, “<Files>”)
      		
      		sParent = TAB + “<Files>”
      		
      	CASE flChangeDir
      		// Directory Addition
      		sParent = Replace(sPath, gsDrivePath + “\”, “”, IgnoreCase)	// Delete the root from the path
      		sParent = UncompleteDir(sParent)
      		AddDirectory(sParent)
      		
      		// Add the "<Files>" directory
      		TableAddChild(TVT_FileTable, gsDrivePath + TAB + sParent, “<Files>”)
      		
      		sParent = TAB + sParent + TAB + “<Files>”
      		
      	CASE flFile
      		// Search for the tree structure of the parent
      		sParent = Replace(sPath, gsDrivePath, “”, IgnoreCase)	// Delete the root from the path
      		sParent = UncompleteDir(sParent)
      		sParent = Replace(sParent, “\”, TAB)
      		sParent += TAB+“<Files>”
      END
      
      // Add the file
      nSubscript = TableAddChild(TVT_FileTable, gsDrivePath + sParent, sFileName, fSize(sPath + sFileName))
      
      // Modify the icon of the added element
      TVT_FileTable[nSubscript]..CollapsedImage = IMG_FILE
      TVT_FileTable[nSubscript]..ExpandedImage = IMG_FILE
      
      // UPD the TreeMap
      sTreestructure = Replace(gsDrivePath + sParent, TAB, “\”)
      sTreestructure = Replace(sTreestructure, “<Files>”, sFileName)
      
      TreeMapAdd(TMAP_File, sTreestructure, TVT_FileTable.COL_SIZE[nSubscript])
      
      
     type : 458752
   -
     name : AddDirectory
     procedure_id : 1219431843448826704
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      //AddDirectory (<sTreeStructure>)
      //
      // Parameters:
      //	sTreeStructure: <specify the role of sTreeStructure>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procedure AddDirectory(sTreestructure)
      
      // Variables
      nParentSubscript, nAddedSubscript is int
      sTreeStructureTMP is string
      nParentSubscript = 1
      sTreestructure = Replace(sTreestructure, “\”, TAB)
      
      FOR EACH string sDirectory of sTreestructure SEPAREE by TAB
      	nParentSubscript = TableSeekChild(COL_NAME, sDirectory, True, nParentSubscript)
      	
      	IF nParentSubscript = -1 THEN
      		nAddedSubscript = TableAddChild(TVT_FileTable, gsDrivePath + [TAB] + sTreeStructureTMP, sDirectory)
      		nParentSubscript = nAddedSubscript
      	ELSE
      		nAddedSubscript = nParentSubscript
      	END
      	
      	// build the tree structure
      	sTreeStructureTMP += [TAB] + sDirectory
      END
     type : 458752
   -
     name : FindTablePosition
     procedure_id : 1219431843448892240
     type_code : 14
     code : |1+
      // Summary: <specify the action of the procedure>
      // Syntax:
      // FindTablePosition (<sPath>)
      //
      // Parameters:
      // 	sPath: <specify the role of sPath>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procedure FindTablePosition(LOCAL sPath)
      
      // Variables
      nParentSubscript is int = 1
      
      // Retrieve the path only
      sPath = fExtractPath(sPath,fDrive+fDirectory) + “<Files>\” + fExtractPath(sPath,fFileName+fExtension)
      
      // Retrieve the beginning of the path
      sPath = Replace(sPath, CompleteDir(gsDrivePath), “”)
      
      // Extract the directory to find
      FOR EACH string sDirectory of sPath SEPAREE by “\”
      	nParentSubscript = TableSeekChild(COL_NAME,sDirectory,True,nParentSubscript)
      END
      
      TableCollapseAll(TVT_FileTable)
      TableSelectPlus(TVT_FileTable,nParentSubscript)
      TablePosition(TVT_FileTable,nParentSubscript)
      
